{
    "target": "esp32s3",
    "builds": [
        {
            "name": "esp32s3-smart-speaker",
            "sdkconfig_append": [
                "CONFIG_ESP_WIFI_ENTERPRISE_SUPPORT=n",
                "CONFIG_LWIP_IPV6=n",
                "CONFIG_USE_ESP_WAKE_WORD=y",
                "CONFIG_USE_AUDIO_DEBUGGER=y",
                "CONFIG_AUDIO_DEBUG_UDP_SERVER=\"192.168.122.143:8000\""
            ]
        }
    ]
}
